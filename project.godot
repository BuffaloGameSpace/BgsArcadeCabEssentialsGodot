config_version=5

[application]

config/name="BGS Arcade Cab Demo"
config/description="The BGS Arcade Cab Plugin, complete with a small demo project"
config/version="1.0"
run/main_scene="res://demo/src/demo/demo.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"
boot_splash/main_scene="res://demo/src/demo/demo.tscn"

[autoload]

BgsCabCredits="*res://addons/bgs_cab_essentials/autoloads/bgs_credits_autoload.gd"
BgsCabIdleQuit="*res://addons/bgs_cab_essentials/autoloads/bgs_idle_quit_autoload.gd"
BgsGeneralConfig="*res://addons/bgs_cab_essentials/autoloads/bgs_general_autoload.gd"

[bgs_arcade_cab]

general/hide_cursor=true
general/force_fullscreen=true
credits/minimum_required_credits=1
credits/free_play_enabled=false
idle_quit/enabled=true
idle_quit/timeout=30

[editor_plugins]

enabled=PackedStringArray("res://addons/bgs_cab_essentials/plugin.cfg")

