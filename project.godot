[bgs_arcade_cab]

general/hide_cursor=true
general/force_fullscreen=true
credits/minimum_required_credits=1
credits/free_play_enabled=false
idle_quit/enabled=true
idle_quit/timeout=30

[editor_plugins]

enabled=PackedStringArray("res://addons/bgs_arcade_cab_essentials/plugin.cfg")

